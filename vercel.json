{
	"version": 2,
	"public": true,
	"builds": [
		{
			"src": "gymshark-mcp-server/src/server.ts",
			"use": "@vercel/node",
			"config": {
				"runtime": "node18.x"
			}
		}
	],
	"routes": [
		{
			"src": "/mcp(.*)",
			"dest": "gymshark-mcp-server/src/server.ts",
			"headers": {
				"Access-Control-Allow-Origin": "*",
				"Access-Control-Allow-Methods": "GET, POST, OPTIONS",
				"Access-Control-Allow-Headers": "Content-Type"
			}
		}
	]
}
