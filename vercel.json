{
	"version": 2,
	"public": true,
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "assets"
			}
		},
		{
			"src": "gymshark-mcp-server/src/server.ts",
			"use": "@vercel/node",
			"config": {
				"runtime": "node18.x"
			}
		}
	],
	"routes": [
		{
			"src": "/mcp(.*)",
			"dest": "gymshark-mcp-server/src/server.ts"
		},
		{
			"src": "/(.*)",
			"dest": "/assets/$1"
		}
	]
}
